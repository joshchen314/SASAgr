1. Merge
  DATA C9_1;
   MERGE C9_1(IN=A)  VIL(IN=B);
   BY TOWN;
   IF A;

2. Table name
  PROC PRINT DATA=crop03_TP label noobs split='/';
    TITLE  '臺北市99年農林漁牧業普查全年農牧戶九大類作物種植家數';
    VAR    HSTNAME SNUM SC1 - SC9;
		LABEL    HSTNAME='行政區'
		         SNUM='作物種植家數'
		         SC1='稻作'
		         SC2='雜糧類'
		         sC3='特用作物類' 
		         sC4='蔬菜類'
		         sC5='果樹類'
		         sC6='食用菇菌類'
		         sC7='甘蔗'
		         sC8='花卉類'
		         sC9='其他農作物';
	         
3.  Dummy variables or produce new variables
 ARRAY dummys{7} COL1 - COL7; 
  DO i=1 TO 7; 
  dummys(i) = 0; 
  END; 
 dummys(D010001) = 1; 
 
4. Local county
if hs in ('71','72') then do;  /*將資料分類*/
      area1=2;
      area2=0;
end;
else do;
       area1=1;
	   if hs in ('65','63','17','18','02','03','04') then area2=1;
	   if hs in ('66','05','07','08','09') then area2=2;
	   if hs in ('67','64','20','10','13','16') then area2=3;
	   if hs in ('14','15') then area2=4;
end;
