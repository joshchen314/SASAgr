/* To read the data from excel file in the server by SAS EG and to save it as SAS library */

proc import datafile="\\saseg\D54\D54B\0_DATA\10308\SS104TT2.xlsx"
   DBMS=XLSX
   out=B102 REPLACE;
      SHEET="農牧場";
   range="A1:Gr21";
   GETNAMES=YES;  
run;
/*PROC PRINT DATA=A102;
RUN;*/

/*步驟1-2*/
/*再用匯出的方式轉成SAS資料檔*/

/*步驟2*/
/*匯出後的資料檔再轉成文字檔*/
